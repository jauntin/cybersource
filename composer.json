{
  "name": "jauntin/cybersource",
  "description": "Laravel package for integrating with CyberSource",
  "keywords": [
    "jauntin",
    "cybersource"
  ],
  "homepage": "https://github.com/jauntin/cybersource",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Sid Momin",
      "email": "sid@jauntin.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.3 <8.4",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "guzzlehttp/guzzle": "^7.9.3",
    "orchestra/testbench": "^8.35.0|^9.5.2|^10.2.1",
    "larastan/larastan": "^2.9.9|^3.4",
    "phpunit/phpunit": "^10.5.45|^11.5.18",
    "spatie/phpunit-snapshot-assertions": "^5.1.8"
  },
  "autoload": {
    "psr-4": {
      "Jauntin\\CyberSource\\": "src",
      "Jauntin\\CyberSource\\Testing\\": "testing"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jauntin\\CyberSource\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jauntin\\CyberSource\\CyberSourceServiceProvider"
      ]
    }
  }
}
