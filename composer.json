{
  "name": "jauntin/cybersource",
  "description": "Laravel package for integrating with CyberSource",
  "keywords": [
    "jauntin",
    "cybersource"
  ],
  "homepage": "https://github.com/jauntin/cybersource",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Sid Momin",
      "email": "sid@jauntin.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1 <8.4",
    "illuminate/support": "^10.0",
    "guzzlehttp/guzzle": "^7.0.0"
  },
  "require-dev": {
    "laravel/pint": "^1.16",
    "orchestra/testbench": "^8.23.2",
    "phpstan/phpstan": "^1.11.4",
    "phpunit/phpunit": "^10.5.20",
    "spatie/phpunit-snapshot-assertions": "^5.1.6"
  },
  "autoload": {
    "psr-4": {
      "Jauntin\\CyberSource\\": "src",
      "Jauntin\\CyberSource\\Testing\\": "testing"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jauntin\\CyberSource\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jauntin\\CyberSource\\CyberSourceServiceProvider"
      ]
    }
  }
}
