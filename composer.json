{
  "name": "jauntin/cybersource",
  "description": "Laravel package for integrating with CyberSource",
  "keywords": [
    "jauntin",
    "cybersource"
  ],
  "homepage": "https://github.com/jauntin/cybersource",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Sid Momin",
      "email": "sid@jauntin.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.8",
    "laravel/pint": "^1.13",
    "orchestra/testbench": "^8.0",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.4",
    "spatie/phpunit-snapshot-assertions": "^5.1.2"
  },
  "autoload": {
    "psr-4": {
      "Jauntin\\CyberSource\\": "src",
      "Jauntin\\CyberSource\\Testing\\": "testing"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jauntin\\CyberSource\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "analyze": "vendor/bin/pint && vendor/bin/phpstan analyse --memory-limit=1G",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jauntin\\CyberSource\\CyberSourceServiceProvider"
      ]
    }
  }
}
